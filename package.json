{
	"name": "create-wih-app",
	"version": "1.0.6",
	"description": "An opinionated type-safe SvelteKit starter",
	"repository": {
		"type": "git",
		"url": "https://github.com/WarningImHack3r/create-wih-app"
	},
	"scripts": {
		"typecheck": "tsc",
		"build": "tsup src",
		"dev": "tsup src --watch",
		"start": "node dist/index.js",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"keywords": [
		"create-wih-app",
		"svelte",
		"sveltekit",
		"typescript",
		"tailwindcss"
	],
	"engines": {
		"node": ">=20.15.0"
	},
	"exports": "./dist/index.js",
	"bin": {
		"create-wih-app": "./dist/index.js"
	},
	"author": "Antoine Lethimonnier <43064022+WarningImHack3r@users.noreply.github.com>",
	"license": "MIT",
	"type": "module",
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"chalk": "^5.3.0",
		"create-svelte": "^6.3.3",
		"tailwindcss": "^3.4.4"
	},
	"devDependencies": {
		"@total-typescript/ts-reset": "^0.5.1",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.14.9",
		"eslint": "^9.6.0",
		"eslint-config-prettier": "^9.1.0",
		"prettier": "^3.3.2",
		"tsup": "^8.1.0",
		"type-fest": "^4.21.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^7.15.0"
	}
}
